algoritmo "Aprovado Reprovado"
var
   N1, N2, M: real

inicio
      Escreval ("Primeira Nota: ")
      Leia (N1)
      Escreva ("Segunda Nota: ")
      Leia (N2)
      M <- (N1 + N2)/ 2
      EscrevaL ("sua media e " , M)
      Se (M >= 7) e (M >= 7) entao
      EscrevaL ("Parabéns, voce esta APROVADO ")
      Senao
      Escreva ("Que pena, voce esta REPROVADO ")
      FimSe






fimalgoritmo
