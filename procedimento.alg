algoritmo "Procedimento"
var
     N: inteiro
Procedimento parouimpar(v: inteiro)
inicio
       se (v%2 = 0) entao
          escreval ("o numero ", V , "e par ")
       senao
            escreval ("o numero " , V , "e impar")
        fimse
fimprocedimento
inicio
      escreval ("digite um numero: ")
      leia (N)
      Parouimpar (N)
fimalgoritmo
