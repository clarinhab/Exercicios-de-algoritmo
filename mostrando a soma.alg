algoritmo "somadornumerico"

var
     Contador, N, S: inteiro
     
inicio
       contador <- 1
       S <- 0
       enquanto (contador <= 5) faca
       contador <- contador + 1
       escreval ("digite um valor: ")
       leia (N)
       S <- S + N
       fimenquanto
       Escreval ("a soma de todos os valores foi: " , S)
fimalgoritmo
